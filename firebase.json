{
  "$schema": "./node_modules/firebase-tools/schema/firebase-config.json",
  "functions": {
    "runtime": "nodejs16"
  },
  "hosting": {
    "site": "ecsact-dev",
    "public": "bazel-bin/src/prodapp",
    "predeploy": "bazel build //src:prodapp",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "redirects": [
      {
        "source": "/ecsact_sdk_0.4.2_windows_x64.zip",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.2/ecsact_sdk_0.4.2_windows_x64.zip",
        "type": 302
      },
      {
        "source": "/ecsact_sdk_0.4.2_linux_x64.tar.gz",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.2/ecsact_sdk_0.4.2_linux_x64.tar.gz",
        "type": 302
      },
      {
        "source": "/ecsact_sdk_0.4.1_windows_x64.zip",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.1/ecsact_sdk_0.4.1_windows_x64.zip",
        "type": 302
      },
      {
        "source": "/ecsact_sdk_0.4.1_linux_x64.tar.gz",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.1/ecsact_sdk_0.4.1_linux_x64.tar.gz",
        "type": 302
      },
      {
        "source": "/ecsact_sdk_0.4.1_windows_x64.msix",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.1/ecsact_sdk_0.4.1_windows_x64.msix",
        "type": 302
      },
      {
        "source": "/ecsact_sdk_0.4.0_windows_x64.msix",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.0/ecsact_sdk_0.4.0_windows_x64.msix",
        "type": 302
      },
      {
        "source": "/ecsact_sdk.appinstaller",
        "destination": "https://github.com/ecsact-dev/ecsact_sdk/releases/download/0.4.1/ecsact_sdk.appinstaller",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
